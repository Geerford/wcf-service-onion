<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
    </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IWcfService" />
        <binding name="BasicHttpsBinding_IWcfService">
          <security mode="Transport" />
        </binding>
      </basicHttpBinding>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IWcfService" />
      </wsHttpBinding>
    </bindings>
    
    <client>
      <endpoint address="https://localhost/WCF.Service/WcfService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_IWcfService"
        contract="WcfService.IWcfService" name="BasicHttpsBinding_IWcfService" />
      <endpoint address="http://localhost:11825/WcfService.svc" binding="wsHttpBinding"
        bindingConfiguration="WSHttpBinding_IWcfService" contract="ServiceReference.IWcfService"
        name="WSHttpBinding_IWcfService">
        <identity>
          <userPrincipalName value="NEVERFORD\nevdev" />
        </identity>
      </endpoint>
    </client>
    
  </system.serviceModel>
</configuration>